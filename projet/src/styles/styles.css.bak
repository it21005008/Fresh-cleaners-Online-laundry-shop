/*footer
}*/
*{
	margin:0px;
	padding:0px;
	box-sizing: border-box;
}
.container{
	max-width:1170px;
	
	margin:auto;
}
.row{
	display:flex;
	flex-wrap:wrap;
	
}
.footer-col{
	width:25%;
	padding:0 15px;
}
.footer-col h5{
	font-size:18px;
	font-family: Century Gothic,  sans-serif;
	color:#ffffff;
	text-transform: capitalize;
	margin-bottom:35px;
	font-weight:500;
	position:relative;
}

.footer-col h5::before{
	content: '';
	position: absolute;
	left:0;
	bottom: -10px;
	background-color: #e91e63;
	height: 2px;
	box-sizing: border-box;
	width: 50px;
}

.footer-col li{
	font-family: Century Gothic,  sans-serif;
	line-height:1.5;
}
.footer-col ul li:not(:last-child){
	margin-bottom: 10px;
}
.footer-col ul li a{
	font-size: 16px;
	text-transform: capitalize;
	color: #ffffff;
	text-decoration: none;
	font-weight: 300;
	color: #bbbbbb;
	display: block;
	transition: all 0.3s ease;
}
.footer-col ul li a:hover{
	color: #ffffff;
	padding-left: 8px;
}
.footer-col .social-links a{
	display: inline-block;
	height: 40px;
	width: 40px;
	background-color: rgba(255,255,255,0.2);
	margin:0 10px 10px 0;
	text-align: center;
	line-height:40px;
	padding-top:10px;
	border-radius:40%;
	color:#ffffff;
	transition: all 0.5s ease;
}
.footer-col .social-links a:hover{
	color: #24262b;
	background-color: #ffffff;
}

