/*header*/
.header{
	background-image: url("../images/img5.jpg");
}
.wrapper {
    display: grid;
    grid-gap: 0px;
   grid-template-columns: 80% 8% 12%;
   
	
  }

  .box {
	 
    
    padding: 0px;
    border-radius: 5px;
	
	
	
  }

  .a {
	  
    grid-column: 1 ;
    grid-row: 1/3;
	
  }
  .b {
    grid-column: 2 ;
    grid-row: 1 ;
  }
  .c {
    grid-column: 2 ;
    grid-row: 2 ;
  }
  .d {
    grid-column: 3;
    grid-row: 1/3;
  }


